<head>
    <script src="~/Content/Assets/js/jquery-3.2.1.min.js"></script>
    <!-- Sweet Alerts js -->
    <script src="~/lib/sweetalert2/sweetalert2.min.js"></script>

    <!-- Sweet alert init js-->
    <script src="~/js/pages/sweet-alerts.init.js"></script>
    <!-- Sweet Alert-->
    <link href="~/lib/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />

</head>


@if ((string)TempData["notificationTitle"] != null)
{
    <script>
        Swal.fire({
            position: 'top-end',
            icon: '@TempData["notificationType"]',
            title: '@TempData["notificationTitle"]',
            text: '@TempData["notificationMessage"]',
            showConfirmationButton: false});
    </script>
}

